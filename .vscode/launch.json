  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch general_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/general_test",
      "args": ["-D"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "build",
      // "preLaunchTask": "build_debug",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch point_light_intensity_calibration_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/point_light_intensity_calibration_test",
      "args": ["-D"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/src/apps/test/point_light_intensity_calibration",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      // "preLaunchTask": "build",
      "preLaunchTask": "build_debug",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
